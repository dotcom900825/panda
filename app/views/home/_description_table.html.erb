<div class="col-md-6 col-sm-12">
  <div class="phighlight ptable-content animated animation bounceIn">
    <%= content_tag(:h3, description[:question])%>
    <%= content_tag(:span, description[:benefit], :class=>'benefit-header')%>
    <ul class="list">
      <li>1200 Pages</li>
      <li>20 Stories</li>
      <li>24/7 Support</li>
    </ul>
    <%= link_to t('front_page.learn_more'), description[:learn_more_url], :class=>'btn btn-primary btn-sm'%>
    <%= link_to t('front_page.create_account'), description[:url], :class=>'btn btn-info btn-sm'%>
  </div>
</div>
